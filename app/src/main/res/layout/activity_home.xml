<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/legacy_primary"
    tools:context="kdas.i_nterface.uitest_2.Home">

    //view14 -- for nearby

    <android.support.v7.widget.CardView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/view10"
        app:cardCornerRadius="8dp"
        app:cardElevation="12dp"
        android:layout_marginTop="70dp"
        android:layout_marginStart="24dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/some_teal2"
            android:layout_marginBottom="40dp">

            <ImageView
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_centerVertical="true"
                android:paddingBottom="20dp"
                android:paddingTop="10dp"
                android:src="@drawable/a_11"/>

        </RelativeLayout>

        </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/view11"
        app:cardCornerRadius="8dp"
        app:cardElevation="12dp"
        android:layout_marginTop="50dp"
        android:layout_alignParentEnd="false"
        android:layout_marginEnd="1dp"
        android:layout_centerHorizontal="true"
        android:clickable="false">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

            <ImageView
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:layout_centerHorizontal="true"

                android:src="@drawable/ic_memories_48dp"/>

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/view12"
        app:cardCornerRadius="8dp"
        app:cardElevation="12dp"
        android:layout_below="@+id/view11"
        android:layout_marginTop="70dp"
        android:layout_alignParentTop="false"
        android:layout_alignParentBottom="false"
        android:layout_centerHorizontal="true">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/ic_friends_48dp"/>

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/view14"
        app:cardCornerRadius="8dp"
        app:cardElevation="12dp"
        android:layout_below="@+id/view12"
        android:layout_marginTop="70dp"
        android:layout_alignParentTop="false"
        android:layout_alignParentBottom="false"
        android:layout_centerHorizontal="true">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_centerHorizontal="true"
                android:src="@drawable/ic_nearby_48dp"/>

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="15dp"
        android:id="@+id/view13"
        app:cardCornerRadius="50dp"
        app:cardElevation="12dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <ImageView
                android:layout_width="100dp"
                android:layout_height="140dp"
                android:layout_centerHorizontal="true"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:src="@drawable/ping_route"/>

        </RelativeLayout>

    </android.support.v7.widget.CardView>





</RelativeLayout>
